<% ql_service = AuthorityService::QualificationLevelsService.new %>
<%= f.input :qualification_level, as: :multi_value_select,
            label: 'Degree Level',
            collection: ql_service.select_all_options,
            include_blank: 'Please select',
            item_helper: ql_service.method(:include_current_value),
            input_html: { class: 'form-control' } %>